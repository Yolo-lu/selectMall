/* pages/addressEdit/addressEdit.wxss */
@import "/app.wxss";
.input{
  margin: 20rpx;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #e2e2e2
}
.address{
  display: flex;
  justify-content: space-between;
  padding: 20rpx;
  font-size: 28rpx
}
.green{
  color: green
}
.bottom{
  position: fixed;
  height: 100rpx;
  line-height: 100rpx;
  background: #B4282D;
  color: #fff;
  bottom: 0;
  left: 0;
  right: 0;
  text-align: center
}