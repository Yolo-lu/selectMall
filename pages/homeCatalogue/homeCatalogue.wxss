/* pages/homeCatalogue/homeCatalogue.wxss */
@import "/app.wxss";
.bg{
  background: #eee;
  position: relative;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.bgcolor{
  margin-top: 30rpx
}
.info{
  background: #fff;
  text-align: center;
  padding: 40rpx 0
}
.tab{
  display: flex;
  background: #fff
}
.c-list{
display: flex;
  flex-wrap: wrap;
}
.names{
  width: 150rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center

}
.list{
  width: 48%;
  margin: 1%;
  background: #fff
}
.list-img{
  display: block;
  width: 100%;
  height: 250rpx
}
.red{
  color: red;
  border-bottom: 4rpx solid red
}